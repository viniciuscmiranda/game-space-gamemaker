    speed = 0; 
    exit;  
}
else speed = spd;

//Hp
switch (hp) {
    case 5: image_index = 1; break;
    case 4: image_index = 2; break;
    case 3: image_index = 3; break;
    case 2: image_index = 4; break;
    case 1: image_index = 5; break;
