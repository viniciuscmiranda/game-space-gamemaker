    if instance_exists(obj_player) {global.scr += scrad;}
    global.hard -= hardad; 
    if (drop == 0) instance_create(x,y,obj_puphp);
    
    if (global.sfx==0) audio_play_sound(snd_kill,1,false);
    
    var i = random_range(1,45);
    repeat (8) {
    shard[i] = instance_create(x,y,obj_eshard);
    shard[i].direction = i;
    i += 45; }
    
    instance_destroy();
}

if instance_exists(obj_player){
    //Shot
    if (alarm[0] =- 1 &amp;&amp; x&lt;1300) {
        b = instance_create(x,y,obj_ebullet);
        b.direction = shotd;
        alarm[0] = shottime;
        
        
        if (global.sfx==0){
            audio_play_sound(snd_shoot,1,false);
            if (audio_is_playing(snd_shoot)){
                audio_sound_gain(snd_shoot,0.2,0.1);
            } else  audio_sound_gain(snd_shoot,0.8,0.1);
    
      }
