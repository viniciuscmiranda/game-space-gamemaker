        if (global.sfx == 0) sound = "ON" else sound = "OFF";
        break;
        }
    case 1: {   
        language = "Português"; 
        if (global.fs == 0) screen = "Ligado" else screen = "Desligado";
        if (global.ms == 0) music = "Ligado" else music = "Desligado"; 
        if (global.sfx == 0) sound = "Ligado" else sound = "Desligado";
        break;
        }
    }
   
if (global.lg &gt; 1) global.lg = 0;
if (global.fs &gt; 1) global.fs = 0;
if (global.sfx &gt; 1) global.sfx = 0;
if (global.ms &gt; 1) global.ms = 0;

//music



if( global.ms == 0 &amp;&amp; room==rm_menu &amp;&amp; !audio_is_playing(snd_menu)) {
