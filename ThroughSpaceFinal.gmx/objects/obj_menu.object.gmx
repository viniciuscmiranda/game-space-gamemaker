    instance_create(x,y,obj_arrow);
}

//record hud animation
if (global.rechud &lt; global.rec) {
    if (!instance_exists(obj_player)){
        if(global.rechud &lt; global.rec){
            global.rechud+=n;
            n++;
        }
    }
    if (global.rechud &gt; global.rec){global.rechud --;}
    if (global.rechud&gt;global.rec) {n=1;}
    global.rechud = clamp(global.rechud,0,global.rec);
    
    if (room != rm_infinity)||(!instance_exists(obj_player)) &amp;&amp; (keyboard_check_pressed(vk_enter)) {
        global.rechud = global.rec;    
        n = 1;
       
    }
}
//ask exit
if (quit==1){
    obj_arrow.y = room_height/2 -48;
    obj_arrow.x = room_width/2 - 290 + (399*opos);
        
    if (keyboard_check_pressed(vk_right)) or (keyboard_check_pressed(ord('D'))) {if global.sfx==0 audio_play_sound(snd_button,0.5,false); opos ++; }
    if (keyboard_check_pressed(vk_left)) or (keyboard_check_pressed(ord('A'))) {if global.sfx==0 audio_play_sound(snd_button,0.5,false); opos --; }
    
    if (opos &gt; 1) opos = 0;
    if (opos &lt; 0) opos = 1;
    
         if (keyboard_check_pressed(vk_enter) || (keyboard_check_pressed(vk_space) )){ {if global.sfx==0 audio_play_sound(snd_click,0.5,false);}
        switch (opos){
            case 0: game_end(); break;
            case 1: quit=0; opos=0; break;
        }
     }
}
else if (quit==0){
if (global.pause == 0) &amp;&amp; (room = rm_infinity)
{     
    background_hspeed[0] = -7;
    background_hspeed[1] = -10;
}
//esc to exit
if (keyboard_check_pressed(vk_escape) &amp;&amp; rm==0 &amp;&amp; quit==0) {quit=1;}
if (keyboard_check_pressed(vk_escape) &amp;&amp; rm==0 &amp;&amp; quit==1) {game_end();}

//visible
if (rm == 5) visible = false; else visible = true;

if (rm == 0){

    //arrow move 
    if (keyboard_check_pressed(vk_up)) or (keyboard_check_pressed(ord('W'))) {if global.sfx==0 audio_play_sound(snd_button,0.5,false);pos --; }
    if (keyboard_check_pressed(vk_down)) or (keyboard_check_pressed(ord('S'))){if global.sfx==0 audio_play_sound(snd_button,0.5,false); pos ++; }
    
    if (pos &gt; 3) pos = 0;
    if (pos &lt; 0) pos = 3;
    
    obj_arrow.y = 454+(60*pos);
    
    if audio_is_playing(snd_button)audio_sound_gain(snd_button,0.4,0.1);
    
    //English
    if (global.lg == 0){
        switch (pos){
            case 0: obj_arrow.x = room_width/2 - 90; break;
            case 1: obj_arrow.x = room_width/2 - 142; break;
            case 2: obj_arrow.x = room_width/2 - 128; break;
            case 3: obj_arrow.x = room_width/2 - 84; break;
        }
    }
    
    //Portuguese
    if (global.lg == 1){
        switch (pos){
            case 0: obj_arrow.x = room_width/2 - 107; break;
            case 1: obj_arrow.x = room_width/2 - 156; break;
            case 2: obj_arrow.x = room_width/2 - 125; break;
            case 3: obj_arrow.x = room_width/2 - 90; break;
        }
    }

    
    //press enter on menu
    if (keyboard_check_pressed(vk_enter))||(keyboard_check_pressed(vk_space)||keyboard_check_pressed(vk_control)){ {if global.sfx==0 audio_play_sound(snd_click,0.5,false);}
        switch (pos){ 
            case 0: rm = 1; break;
            case 1: rm = 2; break;
            case 2: rm = 3; break;
            case 3: quit=1;//game_end();
        }
    } alarm[0] = 3;
} }
//Back
 if (keyboard_check_pressed(vk_escape) &amp;&amp; global.reset==0) { {if global.sfx==0 audio_play_sound(snd_click,0.5,false);}
    global.pause = 0;
    if (rm = clamp(rm,1,3)) rm = 0; 
    if (rm = clamp(rm,4,10)) rm = 1;
   
    if (room != rm_menu) room = rm_menu;
    opos = 0;
    ppos = 0;
    
    background_hspeed = -1;
    with(obj_player) {instance_destroy();}
} 

//FullScreen
if (global.fs == 0) window_set_fullscreen(true);
if (global.fs == 1) window_set_fullscreen(false);

if (keyboard_check_pressed(vk_f4)) {
    global.fs++;
}


//Language
switch (global.lg) {
    case 0: {
        language = "English"; 
        if (global.fs == 0) screen = "ON" else screen = "OFF"; 
