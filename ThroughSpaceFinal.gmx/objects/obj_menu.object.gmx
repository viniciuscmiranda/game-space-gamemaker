    audio_sound_gain(snd_menu,0.2,0.1);
    
} 
if (global.ms == 1) {
    audio_sound_gain(snd_menu,0,150);
} else if (global.ms ==0) {audio_sound_gain(snd_menu,0.2,150);}
if (room != rm_menu) { audio_stop_sound(snd_menu);}



//menu music
if( global.ms == 0 &amp;&amp; rm==5 &amp;&amp; !audio_is_playing(snd_game)) {
    audio_play_sound(snd_game,1,true);
    audio_sound_gain(snd_game,0.2,0.1);
    stpsnd=0;
}  
if (room != rm_infinity) {audio_stop_sound(snd_game);}
//game music
if (global.pause == 1) {
    audio_sound_gain(snd_game,0,150);
    stpsnd++;
    if (stpsnd&gt;120) {audio_pause_sound(snd_game); stpsnd=0;}
} else if (global.pause == 0) {
    
    audio_resume_sound(snd_game);
    audio_sound_gain(snd_game,0.2,150);

}

if !instance_exists(obj_player){
    audio_sound_gain(snd_game,0.03,60);
    
    if audio_is_playing(snd_lose){audio_sound_gain(snd_lose,2,0.1);}
    
}

//score hud
if (global.pause==0){
if (global.scrhud &lt; global.scr) {global.scrhud++;}
if (global.scrhud &gt; global.scr) {global.scrhud--;}

if (global.scr &lt; 0){global.scr = 0;}
if (global.scr = 0){global.scrhud = 0;}
}
